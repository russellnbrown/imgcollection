<UserControl x:Class="WpfCoreTester.SearchTab"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WpfCoreTester"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <TextBox HorizontalAlignment="Left" Margin="34,27,0,0" Name="relativeTo" TextWrapping="Wrap" VerticalAlignment="Top" Width="283"/>
        <TextBox HorizontalAlignment="Left" Margin="34,65,0,0" x:Name="scanSet" TextWrapping="Wrap" VerticalAlignment="Top" Width="283"/>
        <TextBox HorizontalAlignment="Left" Margin="34,103,0,0" x:Name="scanLocation" TextWrapping="Wrap" VerticalAlignment="Top" Width="283"/>
        <Button Content="Scan" Name="scanBtn" HorizontalAlignment="Left" Margin="34,169,0,0" VerticalAlignment="Top" Click="scanBtn_Click"/>
        <Button Content="Append" Name="appendBtn" HorizontalAlignment="Left" Margin="97,169,0,0" VerticalAlignment="Top" Click="appendBtn_Click"/>

    </Grid>
</UserControl>
